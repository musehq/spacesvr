{
  "name": "spacesvr",
  "version": "1.7.2",
  "private": true,
  "description": "An Environment for WebXR Spaces",
  "keywords": [
    "react",
    "spaces",
    "webxr",
    "environment",
    "webvr"
  ],
  "author": "Spaces",
  "license": "MIT",
  "homepage": "https://muse.place",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spacesvr/spacesvr.git"
  },
  "main": "main.cjs.js",
  "module": "main.js",
  "types": "main.d.ts",
  "scripts": {
    "build": "rimraf dist && mkdir dist && rollup -c && yarn copy && tsc && yarn pack-dist",
    "lint": "eslint . --ext .ts,.tsx",
    "process-gltf": "./scripts/process-gltf.sh",
    "eslint": "eslint --fix {src,examples/src}/**/*.{ts,tsx}",
    "copy": "copyfiles package.json readme.md LICENSE dist && json -I -f dist/package.json -e \"this.private=false; this.devDependencies=undefined; this.optionalDependencies=undefined; this.scripts=undefined; this.husky=undefined; this.prettier=undefined; this.jest=undefined; this['lint-staged']=undefined;\"",
    "pack-dist": "cd dist && yarn pack && mv *.tgz .. && cd ..",
    "dev": "yarn --cwd ./examples start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "files": [
    "**"
  ],
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^10.0.27",
    "@juggle/resize-observer": "^3.2.0",
    "@react-three/cannon": "^1.1.0",
    "@react-three/drei": "7.0.6",
    "@react-three/fiber": "^6.0.14",
    "@react-three/xr": "^3.0.1",
    "culori": "^0.18.2",
    "nipplejs": "0.8.1",
    "node-fetch": "^2.6.1",
    "peerjs": "^1.3.1",
    "react-device-detect": "^1.13.1",
    "react-spring": "^9.2.0",
    "valtio": "^0.7.0"
  },
  "peerDependencies": {
    "react": ">=17.0",
    "react-dom": ">=17.0",
    "three": ">=0.127",
    "three-stdlib": ">=2.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@react-three/gltfjsx": "^4.1.5",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@types/node": "^14.0.13",
    "@types/node-fetch": "^2.5.7",
    "@types/peerjs": "^1.1.0",
    "@types/react": "^16.9.36",
    "@types/three": "^0.127.1",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-plugin-glsl": "^1.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "copyfiles": "^2.4.1",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "glslify-loader": "^2.0.0",
    "gltf-pipeline": "^3.0.0",
    "husky": "^4.2.5",
    "json": "^10.0.0",
    "lint-staged": "^10.5.1",
    "prettier": "2.0.5",
    "pretty-quick": "^3.1.0",
    "react": "17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "three": "^0.127.0",
    "typescript": "^4.2.4"
  }
}
